.header{
  margin-top: 150rpx;
  text-align: center;
}
.hasBook{
  padding: 10rpx 25rpx;
}
.list{
  margin-top: 20rpx;
  padding: 0 30rpx;
  background: white;
}
.detailHistory li,.detail li{
  display: block;
}
.detailHistory li,.detail li{
  padding:15rpx 10rpx 15rpx 15rpx;
  margin: 10rpx 0;
  border:1rpx solid black;
  border-radius: 20rpx;
  box-shadow: 4rpx 3rpx 5rpx 0rpx rgba(0,0,0,0.35);
  -webkit-box-shadow: 4rpx 3rpx 5rpx 0rpx rgba(0,0,0,0.35);
  -moz-box-shadow: 4rpx 3rpx 5rpx 0rpx rgba(0,0,0,0.35);
  position: relative;
}
.detail li{
  font-weight: bold;
  color: #000;
}
.cancel{
  position: absolute;
  top: 10rpx;
  right: 20rpx;
}
.line{
  width:100%;
  height:2rpx;
  background:#aaa;
}
.disabled {
  pointer-events: none;
  opacity: 0.5;
}
.userInfoBox{
  position: relative;
  height: 120rpx;
}
.userInfo{
  font-weight: bold;
  position: absolute;
  top:20rpx;
  left: 50%;
  margin-left: -200rpx;
  width: 400rpx;
}
.title{
  font-weight: bold;
  padding: 0 0 0 40rpx;
}